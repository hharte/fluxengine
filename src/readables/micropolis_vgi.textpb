comment: 'Micropolis MetaFloppy 615kB 5.25" single-sided hard-sectored .vgi output'

input {
	flux {
		drive {
			hard_sector_count: 16
		}
	}
}

output {
	image {
		filename: "micropolis.vgi"
		vgi {}
	}
}

decoder {
	micropolis {
		mzos_checksum: false
		ignore_track_byte: false
	}
}

cylinders {
	start: 0
	end: 76
}

heads {
	start: 0
	end: 0
}

